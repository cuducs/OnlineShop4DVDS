<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.1"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">

    <lifecycle>
        <phase-listener>
            g3.custom.phaselistener.AdminSideMenuPhaseListener
        </phase-listener>
    </lifecycle>
    <navigation-rule>
        <description>
            global navigation rules
        </description>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>adminindex</from-outcome>
            <to-view-id>/admin/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>showproduct</from-outcome>
            <to-view-id>/showproduct.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/user/welcome.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/user/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cuslistproduct</from-outcome>
            <to-view-id>/user/listproduct.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cart</from-outcome>
            <to-view-id>/user/cart.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>profile</from-outcome>
            <to-view-id>/member/profile.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <description>
            global navigation rules of admin
        </description>
        <from-view-id>/admin/*</from-view-id>
        <navigation-case>
            <from-outcome>productshow</from-outcome>
            <to-view-id>/admin/product/show.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>productform</from-outcome>
            <to-view-id>/admin/product/form.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>productrecovery</from-outcome>
            <to-view-id>/admin/product/recovery.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>productsearch</from-outcome>
            <to-view-id>/admin/product/search.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ordershow</from-outcome>
            <to-view-id>/admin/order/order.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>fileshow</from-outcome>
            <to-view-id>/admin/file/show.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>fileform</from-outcome>
            <to-view-id>/admin/file/form.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>filerecovery</from-outcome>
            <to-view-id>/admin/file/recovery.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>filesearch</from-outcome>
            <to-view-id>/admin/file/search.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>albumshow</from-outcome>
            <to-view-id>/admin/album/show.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>albumform</from-outcome>
            <to-view-id>/admin/album/form.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>albumrecovery</from-outcome>
            <to-view-id>/admin/album/recovery.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>albumsearch</from-outcome>
            <to-view-id>/admin/album/search.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>songshow</from-outcome>
            <to-view-id>/admin/song/show.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>songform</from-outcome>
            <to-view-id>/admin/song/form.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>songrecovery</from-outcome>
            <to-view-id>/admin/song/recovery.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>songsearch</from-outcome>
            <to-view-id>/admin/song/search.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/album/addsong.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>details</from-outcome>
            <to-view-id>/admin/song/details.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/song/details.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addsong</from-outcome>
            <to-view-id>/admin/album/addsong.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/category/addcategory.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>show</from-outcome>
            <to-view-id>/admin/category/show.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/category/show.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/admin/category/addcategory.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
