        </navigation-case>
        <navigation-case>
            <from-outcome>filesearch</from-outcome>
            <to-view-id>/admin/file/search.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gameshow</from-outcome>
