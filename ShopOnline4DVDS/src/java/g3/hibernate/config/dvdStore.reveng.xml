<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="DVDStore" match-schema="dbo"/>
  <table-filter match-name="Genre"/>
  <table-filter match-name="Category"/>
  <table-filter match-name="Movie"/>
  <table-filter match-name="Order"/>
  <table-filter match-name="Song"/>
  <table-filter match-name="Artist"/>
  <table-filter match-name="AlbumMusicMapping"/>
  <table-filter match-name="Album"/>
  <table-filter match-name="Comment"/>
  <table-filter match-name="File"/>
  <table-filter match-name="OrderDetail"/>
  <table-filter match-name="Feedback"/>
  <table-filter match-name="DVD"/>
  <table-filter match-name="Member"/>
  <table-filter match-name="Game"/>
</hibernate-reverse-engineering>
